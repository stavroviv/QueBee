                                                            </valueFactory>
                                                        </Spinner>
                                                        <!--                                          <ChoiceBox fx:id="dfgdfgdfg" layoutY="117.0" prefHeight="25.0" prefWidth="753.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" -->
